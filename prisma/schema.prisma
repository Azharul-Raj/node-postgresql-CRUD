datasource db {
  provider = "postgresql"
  url      = "postgresql://USERNAME:PASSWORD@localhost:5432/DATABASE_NAME"
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id        Int      @id @default(autoincrement())
  name      String
  email     String   @unique
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}

